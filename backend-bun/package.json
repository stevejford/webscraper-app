{
  "name": "webscraper-backend-bun",
  "version": "1.0.0",
  "description": "Bun backend for web scraper with Drizzle ORM and Supabase integration",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun src/db/seed.ts",
    "type-check": "bun --no-install tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "zod": "^3.22.4",
    "hono": "^3.12.0",
    "@hono/node-server": "^1.8.0",
    "openai": "^4.20.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.0",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.4",
    "ws": "^8.16.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pdf-parse": "^1.1.4",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.20.6",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "webscraper",
    "bun",
    "drizzle",
    "supabase",
    "hono",
    "typescript"
  ],
  "author": "Web Scraper Team",
  "license": "MIT"
}
